    "bootstrap-vue": "^2.0.0",
    "cross-env": "^6.0.3",
    "highlight.js": "^9.15.10",
    "koa": "^2.6.2",
    "marked": "^0.7.0",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "nodemon": "^1.18.9",
    "push-dir": "^0.4.1",
    "vue-jest": "^4.0.0-0"
  }
